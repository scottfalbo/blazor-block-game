@page "/"

<h1>Blazor Block Game</h1>

<table>
    @for (int i = 0; i < X; i++)
    {
    <tr>
        @for (int j = 0; j < Y; j++)
        {
            @if (GameBoard[i, j] != null)
            {
                <td class="game-cell">
                    @GameBoard[i, j].Id
                </td>
            }
        }
    </tr>
    }
</table>

